#: src/dotnew:35 src/dotnew-gtk.ui:64
#, sh-format
msgid "Cancel"
msgstr "Άκυρο"

#: src/dotnew:37
#, sh-format
msgid "New settings file found: $file\\n\\nWhat do you want to do with it?"
msgstr "Βρέθηκε νέο αρχείο ρυθμίσεων: $file\\n\\nΤι επιθυμείτε να κάνετε με αυτό;"

#: src/dotnew:38 src/dotnew:54 src/dotnew-gtk:344
#, sh-format
msgid "Replace"
msgstr "Αντικατάσταση"

#: src/dotnew:38
#, sh-format
msgid "Replace the old settings file with the new one"
msgstr "Αντικατάσταση του παλιού αρχείου ρυθμίσεων με το νέο"

