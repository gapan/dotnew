#: src/dotnew:20
#, sh-format
msgid "You need to be root to run this"
msgstr "Πρέπει να έχετε δικαιώματα υπερχρήστη (root)"

#: src/dotnew:33 src/dotnew:44 src/dotnew:100 src/dotnew:103
#, sh-format
msgid "OK"
msgstr "Εντάξει"

#: src/dotnew:33 src/dotnew:44
#, sh-format
msgid "Cancel"
msgstr "Άκυρο"

#: src/dotnew:35 src/dotnew:46
#, sh-format
msgid "New settings file found: $file\\n\\nWhat do you want to do with it?"
msgstr "Βρέθηκε νέο αρχείο ρυθμίσεων: $file\\n\\nΤι επιθυμείτε να κάνετε με αυτό;"

#: src/dotnew:36 src/dotnew:47 src/dotnew:62 src/dotnew-gtk:207
#, sh-format
msgid "Replace"
msgstr "Αντικατάσταση"

#: src/dotnew:36 src/dotnew:47
#, sh-format
msgid "Replace the old settings file with the new one"
msgstr "Αντικατάσταση του παλιού αρχείου ρυθμίσεων με το νέο"

#: src/dotnew:37 src/dotnew:48 src/dotnew:67 src/dotnew-gtk:209
#, sh-format
msgid "Delete"
msgstr "Διαγραφή"

#: src/dotnew:37 src/dotnew:48
#, sh-format
msgid "Delete the new settings file"
msgstr "Διαγραφή του νέου αρχείου ρυθμίσεων"

#: src/dotnew:38 src/dotnew:49 src/dotnew:70 src/dotnew-gtk.glade:38
#, sh-format
msgid "Diff"
msgstr "Διαφορές"

#: src/dotnew:38 src/dotnew:49
#, sh-format
msgid "View the differences between the old and new settings files"
msgstr "Εμφάνιση των διαφορών μεταξύ του παλιού και του νέου αρχείου"

#: src/dotnew:39
#, sh-format
msgid "Use vimdiff to edit the old and new settings files"
msgstr "Χρήση του vimdiff για επεξεργασία του παλιού και του νέου αρχείου"

#: src/dotnew:40 src/dotnew:50
#, sh-format
msgid "Nothing"
msgstr "Τίποτα"

#: src/dotnew:40 src/dotnew:50
#, sh-format
msgid "Do nothing for now, decide about it later"
msgstr "Τίποτα προς το παρόν, αφήστε το για αργότερα"

#: src/dotnew:90
#, sh-format
msgid "Yes"
msgstr "Ναι"

#: src/dotnew:91
#, sh-format
msgid "No"
msgstr "Όχι"

#: src/dotnew:92
#, sh-format
msgid "There is no $orig file present.\\nMove $i to $orig ?"
msgstr "Δεν υπάρχει αρχείο $orig.\\nΜεταφορά του $i στο $orig ;"

#: src/dotnew:99
#, sh-format
msgid "Ready"
msgstr "Τέλος"

#: src/dotnew:101
#, sh-format
msgid "Done processing all .new files"
msgstr "Έγινε επεξεργασία όλων των αρχείων .new"

#: src/dotnew:103 src/dotnew-gtk.glade:384
#, sh-format
msgid "No .new files"
msgstr "Μη ύπαρξη αρχείων .new"

#: src/dotnew:104 src/dotnew-gtk.glade:403
#, sh-format
msgid "There are no .new configuration files in your system"
msgstr "Δεν υπάρχουν αρχεία ρυθμίσεων .new στο σύστημα"

#: dotnew.desktop.in.h:1 dotnew-kde.desktop.in.h:1
msgid "Manage .new system settings files"
msgstr "Διαχείριση νέων αρχείων ρυθμίσεων συστήματος .new"

#: src/dotnew-gtk.glade:33
msgid "Refresh configuration file list"
msgstr "Ανανέωση λίστας αρχείων ρυθμίσεων"

#: src/dotnew-gtk.glade:37
msgid "Compare files line by line"
msgstr "Σύγκριση αρχείων γραμμή προς γραμμή"

#: src/dotnew-gtk.glade:51
msgid "Use gvimdiff or vimdiff to merge diffs"
msgstr "Χρήση του gvimdiff ή του vimdiff για τη συγχώνευση των διαφορών"

#: src/dotnew-gtk.glade:65
msgid "Use meld to merge diffs"
msgstr "Χρήση του meld για τη συγχώνευση των διαφορών"

#: src/dotnew-gtk.glade:88
msgid "Mark all configuration files for replacement"
msgstr "Επισήμανση όλων των αρχείων ρυθμίσεων για αντικατάσταση"

#: src/dotnew-gtk.glade:89
msgid "Replace all"
msgstr "Αντικατάσταση όλων"

#: src/dotnew-gtk.glade:102
msgid "Mark all new configuration files for deletion"
msgstr "Επισήμανση όλων των νέων αρχείων ρυθμίσεων για διαγραφή"

#: src/dotnew-gtk.glade:103
msgid "Delete all"
msgstr "Διαγραφή όλων"

#: src/dotnew-gtk.glade:116
msgid "Unmark all checkboxes"
msgstr "Αναίρεση της επισήμανσης για όλα τα αρχεία ρυθμίσεων"

#: src/dotnew-gtk.glade:117
msgid "Unmark all"
msgstr "Αποεπιλογή όλων"

#: src/dotnew-gtk.glade:139
msgid "Refresh"
msgstr "Ανανέωση"

#: src/dotnew-gtk.glade:455
msgid "A utility to manage .new system configuration files"
msgstr "Διαχείριση νέων αρχείων ρυθμίσεων συστήματος .new"

#: src/dotnew-gtk.glade:1133
msgid "translator-credits"
msgstr "Γιώργος Βλαχάβας <vlahavas~at~gmail~dot~com>"

