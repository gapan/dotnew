#: src/dotnew:20
#, sh-format
msgid "You need to be root to run this"
msgstr "Vous devez posséder le droits d'administrateur (root) afin d'exécuter ce programme"

#: src/dotnew:33 src/dotnew:44 src/dotnew:100 src/dotnew:103
#, sh-format
msgid "OK"
msgstr "Valider"

#: src/dotnew:33 src/dotnew:44
#, sh-format
msgid "Cancel"
msgstr "Annuler"

#: src/dotnew:35 src/dotnew:46
#, sh-format
msgid "New settings file found: $file\\n\\nWhat do you want to do with it?"
msgstr "Un nouveau fichier de configuration est disponible: $file\\n\\nQue voulez vous faire ?"

#: src/dotnew:36 src/dotnew:47 src/dotnew:62 src/dotnew-gtk:176
#, sh-format
msgid "Replace"
msgstr "Remplacer"

#: src/dotnew:36 src/dotnew:47
#, sh-format
msgid "Replace the old settings file with the new one"
msgstr "Remplacer l'ancien fichier de configuration par le nouveau"

#: src/dotnew:37 src/dotnew:48 src/dotnew:67 src/dotnew-gtk:178
#, sh-format
msgid "Delete"
msgstr "Supprimer"

#: src/dotnew:37 src/dotnew:48
#, sh-format
msgid "Delete the new settings file"
msgstr "Supprimer le nouveau fichier de configuration"

#: src/dotnew:38 src/dotnew:49 src/dotnew:70 src/dotnew-gtk.glade:40
#, sh-format
msgid "Diff"
msgstr "Diff"

#: src/dotnew:38 src/dotnew:49
#, sh-format
msgid "View the differences between the old and new settings files"
msgstr "Afficher les différences entre l'ancien et le nouveau fichier de configuration"

#: src/dotnew:39
#, sh-format
msgid "Use vimdiff to edit the old and new settings files"
msgstr "Utiliser vimdiff afin de modifier l'ancien et le nouveau fichier de configuration"

#: src/dotnew:40 src/dotnew:50
#, sh-format
msgid "Nothing"
msgstr "Rien"

#: src/dotnew:40 src/dotnew:50
#, sh-format
msgid "Do nothing for now, decide about it later"
msgstr "Ne rien faire pour l'instant, décider plus tard"

#: src/dotnew:90
#, sh-format
msgid "Yes"
msgstr "Oui"

#: src/dotnew:91
#, sh-format
msgid "No"
msgstr "Non"

#: src/dotnew:92
#, sh-format
msgid "There is no $orig file present.\\nMove $i to $orig ?"
msgstr "Aucun fichier $orig n'a été trouvé.\\nTransformer $i en $orig ?"

#: src/dotnew:99
#, sh-format
msgid "Ready"
msgstr "Action terminée"

#: src/dotnew:101
#, sh-format
msgid "Done processing all .new files"
msgstr "Tous les fichiers .new ont été parcourus"

#: src/dotnew:103
#, sh-format
msgid "No .new files"
msgstr "Aucun fichier .new"

#: src/dotnew:104
#, sh-format
msgid "There are no .new configuration files in your system"
msgstr "Il n'y a aucun nouveau fichier de configuration .new dans votre système"

#: dotnew.desktop.in.h:1 dotnew-kde.desktop.in.h:1
msgid "Manage .new system settings files"
msgstr "Gérer les fichiers configuration système '.new'"

#: src/dotnew-gtk.glade:35
msgid "Refresh configuration file list"
msgstr "Rafraîchir la liste des fichiers de configuration"

#: src/dotnew-gtk.glade:39
msgid "Compare files line by line"
msgstr "Comparer les fichiers ligne par ligne"

#: src/dotnew-gtk.glade:53
msgid "Use gvimdiff or vimdiff to merge diffs"
msgstr "Utilisez gvimdiff ou vimdiff pour fusionner les différences"

#: src/dotnew-gtk.glade:76
msgid "Mark all configuration files for replacement"
msgstr "Marquer tous les fichiers de configuration comme \"à remplacer\""

#: src/dotnew-gtk.glade:77
msgid "Replace all"
msgstr "Les remplacer tous"

#: src/dotnew-gtk.glade:90
msgid "Mark all new configuration files for deletion"
msgstr "Marquer tous les nouveaux fichiers de configuration comme \"à supprimer\""

#: src/dotnew-gtk.glade:91
msgid "Delete all"
msgstr "Les supprimer tous"

#: src/dotnew-gtk.glade:104
msgid "Unmark all checkboxes"
msgstr "Retirer toutes les marques"

#: src/dotnew-gtk.glade:105
msgid "Unmark all"
msgstr "Tout retirer"

#: src/dotnew-gtk.glade:127
msgid "Refresh"
msgstr "Rafraîchir"

#: src/dotnew-gtk:111
msgid " vs "
msgstr "contre"

#: src/dotnew-gtk:180
msgid "Configuration file"
msgstr "Fichier de configuration"

